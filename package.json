		"jsoncodegen": "../jsoncodegen-cli",
		"jsoncodegen-test-json": "^1.1.0",
		"jsoncodegen-types-for-generator": "^1.0.1",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.3"
	},
	"scripts": {
		"compile": "rimraf dist && tsc -p .",
		"watch": "rimraf dist && tsc -p . -w",
